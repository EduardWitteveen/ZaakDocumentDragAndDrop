<documentmapping>  
  <zaaktype matchfield="code" matchvalue="B1026">
    <document matchfield="naam" matchvalue="Minimaregelingen en kindpakket.pdf">
      <type>Fomulier</type>
      <vertrouwelijkheid>Vertrouwelijk</vertrouwelijkheid>
    </document>
    <!--catch all document-->
    <document>
      <!-- <titel>Bijlagen/Bewijsstukken</titel> -->
      <type>Overige</type>
      <vertrouwelijkheid>Vertrouwelijk</vertrouwelijkheid>
    </document>
  </zaaktype>
  <!--catch all zaaktype-->
  <zaaktype>
    <document matchfield="naam" matchvalue="vertrouwelijk">
      <vertrouwelijkheid>Vertrouwelijk</vertrouwelijkheid>
    </document>
    <document>
      <vertrouwelijkheid>Intern</vertrouwelijkheid>
    </document>
  </zaaktype>  
